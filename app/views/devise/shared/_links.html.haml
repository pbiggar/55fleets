.container
  .col-xs-12.col-lg-s.col-xs-offset-0.col-lg-offset-3
    %h1 Sign In With Your Git Account
    - if devise_mapping.omniauthable?
      - resource_class.omniauth_providers.each do |provider|
        .row
          .oauth-login
            = link_to  omniauth_authorize_path(resource_name, provider),'data-no-turbolink' => true do
              -# = t('.sign_in_with_provider', :provider => provider.to_s.titleize, :default => "Sign in with #{provider.to_s.titleize}")
              = image_tag("#{provider}.png",:height => "100px")

